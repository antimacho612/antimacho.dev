  "ignorePaths": [
    "cache",
    "dist",
    "node_modules",
    ".husky",
    ".vscode",
    ".cspell.json",
    ".editorconfig",
    ".eslintrc.cjs",
    ".gitignore",
    ".markdownlint-cli2.jsonc",
    ".prettierrc.cjs",
    ".prettierignore",
    ".textlintrc.json",
    "package.json",
    "package.lock.json",
    "renovate.json",
    "tsconfig.json"
  ]
