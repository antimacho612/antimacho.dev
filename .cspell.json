  "words": [
    "antimacho",
    "Buzan",
    "composables",
    "Decorational",
    "gantt",
    "inputtext",
    "linkify",
    "maskbg",
    "materialdesignicons",
    "mindmap",
    "nofollow",
    "noopener",
    "noreferrer",
    "Popularisation",
    "primevue",
    "Qiita",
    "Regexper",
    "sankey",
    "shiki",
    "tabmenu",
    "tabview",
    "ufeff",
    "uhhhh",
    "verifymethod",
    "vitepress",
    "vuetify",
    "vueuse",
    "xychart",
    "zenn"
