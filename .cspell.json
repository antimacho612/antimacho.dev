// https://cspell.org/configuration/
{
  "version": "0.2",
  "language": "en",
  "ignoreRegExpList": ["(https?|file|ftp):[^\\s]*$"],
  "words": [
    "antimacho",
    "composables",
    "Decorational",
    "inputtext",
    "linkify",
    "maskbg",
    "materialdesignicons",
    "nofollow",
    "noopener",
    "noreferrer",
    "primevue",
    "Qiita",
    "shiki",
    "tabmenu",
    "tabview",
    "vitepress",
    "vuetify",
    "vueuse",
    "zenn"
  ],
  "ignorePaths": [
    "cache",
    "dist",
    "node_modules",
    ".husky",
    ".vscode",
    ".cspell.json",
    ".editorconfig",
    ".eslintrc.cjs",
    ".gitignore",
    ".markdownlint-cli2.jsonc",
    ".prettierrc.cjs",
    ".prettierignore",
    ".textlintrc.json",
    "package.json",
    "package.lock.json",
    "renovate.json",
    "tsconfig.json"
  ]
}
