{
  "name": "antimacho.dev",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vitepress dev",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "lint:eslint": "eslint \".vitepress/theme/**/*.{ts,mts,vue}\" --fix",
    "lint:markdown": "markdownlint-cli2 \"posts/**/*.md\"",
    "lint:text": "textlint \"posts/**/*.md\"",
    "prettier": "prettier . --check --write",
    "prepare": "husky"
  },
  "lint-staged": {
    ".vitepress/theme/**/*.{ts,mts,vue}": [
      "prettier",
      "eslint"
    ],
    "posts/**/*.md": [
      "node scripts/updateFrontmatter",
      "textlint",
      "markdownlint-cli2"
    ]
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.0",
    "@types/node": "^20.11.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cspell": "^8.8.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "gray-matter": "^4.0.3",
    "husky": "^9.0.7",
    "lint-staged": "^15.2.0",
    "markdown-it": "^14.0.0",
    "markdownlint-cli2": "^0.12.1",
    "mermaid": "^10.7.0",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "textlint": "^14.0.0",
    "textlint-rule-ja-no-redundant-expression": "^4.0.1",
    "textlint-rule-ja-unnatural-alphabet": "^2.0.1",
    "textlint-rule-no-doubled-joshi": "^5.1.0",
    "textlint-rule-preset-ja-spacing": "^2.3.0",
    "textlint-rule-prh": "^6.0.0",
    "vitepress": "^1.0.0-rc",
    "vitepress-plugin-mermaid": "^2.0.16",
    "vue": "^3.4.15"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "primevue": "^3.48.1"
  }
}
